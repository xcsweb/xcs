  .tab4card .title {
    display: flex;
    justify-content: space-between;
    color: #333;
    font-size: 34rpx;
    background: white;
    padding: 20rpx 20rpx 20rpx 20rpx;
    align-items: center; }
    .tab4card .title text {
      color: #999; }
    .tab4card .title .cls1 {
      display: inline-flex;
      align-items: center;
      flex: 1;
      justify-content: center;
      font-size: 30rpx; }
    .tab4card .title .blk {
      display: inline-block;
      width: 8rpx;
      height: 30rpx;
      background: #ff9b7a;
      margin-right: 10rpx; }
