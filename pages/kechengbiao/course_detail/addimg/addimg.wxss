page{
  background: #fff;
}
.top{
  background: #F7F7F7;
  height: 20rpx;
}
.imgs{
  padding: 15rpx;
}
.imgs .addImg{
  width: 30%;
  height: 160rpx;
  border: 1px dashed #f1f1f1;
  text-align: center;
  overflow: hidden;
  display: inline-block;
  position: relative;
}
.imgs .addImg .addIcon{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 44%;
  top: 65rpx;
}
.imgs .imgWrapper{
  width: 30%;
  height: 160rpx;
  display: inline-block;
  position: relative;
}
.imgs .imgWrapper:nth-child(3n+3){
  margin-left:20rpx;
}
.imgs .imgWrapper:nth-child(3n+2){
  margin-left:20rpx;
}
.imgs .img{
  width:100%;
  height: 160rpx;
}
.imgs .delete{
  width: 14px;
  height: 14px;
  position: absolute;
  top: -8px;
  right: -5px;
  background: white;
  border-radius: 100px;
  font-size: 6px;
  color: #999;
}

.btnWrapper {
  padding: 15px;
  margin-top: 30%;
}
