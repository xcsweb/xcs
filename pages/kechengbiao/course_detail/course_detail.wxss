/**顶部tabs样式**/

.tabs {
  font-size: 34rpx;
  text-align: center;
  color: #999;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.tab {
  width: 25%;
  display: inline-flex;
  background: white;
  flex-direction: column;
  align-items: center;
  padding: 10rpx 0rpx;
}

.tab .bottomBorder {
  height: 1px;
  width: 30rpx;
  background: #555;
}

/**tab1样式**/

.tab1content .brife {
  color: #333;
  font-size: 34rpx;
  background: white;
  padding: 10rpx;
  line-height: 60rpx;
}

.tab1content .brife .btn{
  border: 1px solid #ccc;
  background: #f1f1f1;
  color: #333;
  font-size: 26rpx;
  padding: 0rpx 6rpx;
  display: inline-block;
  float:right;
  border-radius: 6rpx;
}
.tab1content .brife text{
  color: #666;
}
.tab1content .aa {
  padding: 10rpx;
  background: white;
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
}

.tab1content picker {
  float: right;
}

.tab1content .pickerview {
  white-space: nowrap;
  border: 1rpx solid #ccc;
  padding: 5rpx 8rpx;
  border-radius: 10rpx;
  background: #eee;
  font-size: 34rpx;
  color: #333;
}

.tab1content .commentItem {
  display: flex;
  padding: 20rpx;
  background: white;
}

.tab1content .commentItem .left {
  flex: 15%;
}

.tab1content .commentItem .left image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 200rpx;
}

.tab1content .commentItem .right {
  flex: 85%;
}

.tab1content .commentItem .right .nameArea {
  font-size: 34rpx;
  color: #333;
}

.tab1content .commentItem .right .nameArea .stars {
  float: right;
}

.tab1content .commentItem .right .nameArea .stars image {
  width: 25rpx;
  height: 25rpx;
}

.tab1content .commentItem .date {
  color: #999;
  font-size: 26rpx;
}

.tab1content .commentItem .content {
  color: #666;
  font-size: 30rpx;
}

/**tab2样式**/

.tab2content .teacherItem {
  padding: 20rpx;
  background: white;
  border-bottom: 1px solid #f2f2f2;
}

.teacherItem .gender {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}

.teacherItem .name {
  font-size: 34rpx;
  color: #333;
}

.teacherItem .position {
  font-size: 30rpx;
  color: #666;
}

.teacherItem .status {
  font-size: 34rpx;
  color: #fff;
  background: #6cf;
  padding: 4rpx 12rpx;
  border-radius: 10rpx;
  float: right;
  vertical-align: middle;
}

.tab2content .personCounts {
  font-size: 30rpx;
  color: #333;
  padding: 20rpx;
  background: white;
  margin-top: 20rpx;
}

.personCounts view {
  width: 25%;
  display: inline-block;
}

.tab2content .studentsStatusTable {
  text-align: center;
  margin-top: 20rpx;
  background: white;
}

.studentsStatusTable .row {
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
  color: #333;
  padding: 15rpx 0rpx;
  border-bottom: 1px solid #eee;
  text-align: center;
}

.row>view {
  flex: 18%;
  align-items: center;
}
.row .stuname{
  font-size: 22rpx;
  color: #333;
}
.row .col1{
  position: relative;
}
.row .studentRole{
  position: absolute;
  left: 15rpx;
  top: 0rpx;
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #66d566;
  color: #66d566!important;
  text-align: center;
  border-radius: 100%;
  line-height:32rpx;
  background: white;
}
.row .avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
}
/*
.row>view:nth-child(5n+4) {
  flex: 28%;
}
*/
.studentsStatusTable .header {
  font-size: 30rpx;
  color: #333;
  line-height: 34rpx;
  z-index: 1;
  transition: all ease 1s;
  background:white;
  width: 100%;
  top: 0px;
}

.studentsStatusTable .num {
  font-size: 22rpx;
  color: #333;
}

.studentsStatusTable .studentRole {
  font-size: 22rpx;
  color: #999;
}

.studentsStatusTable .circle {
  width: 50rpx;
  height: 50rpx;
  border-radius: 100rpx;
  border: 1px solid #eee;
  display: inline-flex;
  justify-content: center;
  align-items: center;
}
.studentsStatusTable  .undot{
  width: 20rpx;
  height: 20rpx;
  border-radius: 100%;
  background: #ffffff
}
.studentsStatusTable  .dot{
  width: 20rpx;
  height: 20rpx;
  border-radius: 100%;
}
.studentsStatusTable .circle0 .dot{
  background: #A8A8A8
}
.studentsStatusTable .circle1 .dot{
  background: #FF7547
}
.studentsStatusTable .circle2 .dot{
  background: #66CCFF
}
.studentsStatusTable .circle3 .dot{
  background: #30D130
}
.studentsStatusTable .circle4 .dot{
  background: #A375FF
}

