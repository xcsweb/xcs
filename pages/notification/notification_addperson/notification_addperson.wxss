checkbox .wx-checkbox-input{
   border-radius: 100px;
   overflow: hidden;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked{
   border: 1px solid #FF7547;

}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
   color:#FF7547; /* 对勾颜色 白色 */
   text-align: center;
}
.weui-cells{
  margin-top: 10px;
}
.icon-arrow{
  transform: rotate(90deg)
}
.weui-cell__ft_in-access{
  width: 50px;
  display: inline-block;
  min-width: 50px;
  height: 30px;
  min-height: 30px;
}
.footer{
  padding: 10px;
  display: flex;
  align-items: center;
  background: white;
  position: fixed;
  bottom: 0px;
  width: 100%;
  justify-content: space-around;
  font-size: 34rpx;
  color: #333;
}
.footer button{
  width: 70%;
  margin-top: -2px;
}
.position{
  color: #666;
  font-size: 34rpx;
  padding-left: 40px;
  background: #F3F3F7;
  padding-top: 10px;
  padding-bottom: 10px;
}
.weui-cells:last-child{
  margin-bottom: 100px;
}
.userName{
  color: #666;
  font-size: 34rpx;
}

.className{
  color: #333;
  font-size: 34rpx;
}