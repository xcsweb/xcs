.calendarPicker{
  font-size: 34rpx;
  color: #333;
}
.calendarPicker view{
  border: 1rpx solid #f1f1f1;
  border-radius: 10rpx;
  height: 46rpx;
  line-height: 46rpx;
  padding: 3rpx 6rpx;
}
.topBtnArea{
  text-align: center;
  background: white;
  padding-top: 16rpx;
  margin-bottom: 20rpx;
}
.topBtnArea button::after{
  border: none;
}
.topBtnArea button{
  border-radius: 8rpx;
  box-sizing: border-box;
}
.topBtnArea button:first-child{
  margin-right: 20rpx;
}

.topBtnArea button:last-child{
  margin-left: 20rpx;
}
.listItem{
  background: white;
  padding: 20rpx;
}
.listItem .title{
  font-size: 34rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.listItem .imgArea{
  position: relative;
  width: 100%;
  height: 300rpx;
  overflow: hidden;
}
.listItem .imgArea .status{
  position: absolute;
  top: 0px;
  right: 0px;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  background: #FEA8A8;
  color: white;
  font-size: 30rpx;
  padding: 0rpx 10rpx;
  box-shadow: 0px 4rpx 10rpx #999;
}
.listItem .imgArea image{
  width: 100%;
  height: 366rpx;
  background: #CCCCCC;
}