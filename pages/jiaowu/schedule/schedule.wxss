/* pages/jiaowu/schedule/schedule.wxss */

.btn_list {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  background: #fff;
}

.btn_list .add {
  margin: 0;
  line-height: 60rpx;
  font-size: 30rpx;
  color: #666;
  border-color: #dfdfdf;
  padding-left: 60rpx;
  background-repeat: no-repeat;
  background-size: 36rpx;
  background-position: 15rpx 50%;
}

.btn_list .btn_li {
  display: flex;
  border: 1px solid #dfdfdf;
  border-radius: 10rpx;
}

.btn_list .btn_li image {
  padding: 10rpx 20rpx;
  width: 36rpx;
  height: 36rpx;
}

.btn_list .btn_li image:hover {
  background: #f7f7f7;
}

.btn_list .btn_li image:first-child {
  border-right: 1px solid #dfdfdf;
}
.calendar{
  margin: 20rpx 0;
  background: #fff;
}
.calendar_body{
  display: flex;
  flex-wrap: wrap;
  margin: 0 auto;
}
.calendar_body .dateBox{
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 34rpx;
  color: #333;
  border-bottom: 1px solid #F2F2F2;
}
.calendar_body>view{
  height: 30px;
  line-height: 30px;
  text-align: center;
  font-size: 26rpx;
  color: #333;
}
.dateOn{
  border-radius: 50%;
  background-color: #FFBB00;
}

