/* pages/jiaowu/teacheval/teacheval.wxss */
.teacheval{
  display: flex;
  align-items: center;
  padding: 20rpx;
  background: #fff;
}
.body{
  flex: 1;
  font-size: 34rpx;
  color: #333;
}
.body>view{
  display: flex;
}
.body .mess{
  color: #666;
}
.body .star{
  width: 25rpx;
  height: 24rpx;
}
.teacheval {
  border-bottom:1px solid #f5f5f5;
}
.teacheval .edit{
  font-size: 34rpx;
  line-height: 1;
  padding: 15rpx;
}
.teacheval .edited{
  font-size: 34rpx;
  line-height: 1;
  color: #ff6633;
}