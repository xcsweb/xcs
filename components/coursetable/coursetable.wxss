.wrapper{
  display: flex;
  background: white;
  color: #333;
  font-size: 30rpx;
  box-sizing: border-box;
}
.wrapper .hours{
  width: 135rpx;
  border-right: 1rpx solid #f1f1f1;
  overflow: hidden;
}
.wrapper .hours .hour{
  height: 85rpx;
  line-height: 85rpx;
  border-bottom: 1rpx solid #f1f1f1;
  text-align: center;
}
.coursewrapper{
  flex: 1;
  position: relative;
}
.lines{
  width: 100%;
  height: 85rpx;
  border-bottom: 1rpx solid #f1f1f1;
}
.course{
  width: 100rpx;
  min-height: 20rpx;
  position: absolute;
  background: #FFCC66;
  color: #333;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  text-overflow: ellipsis;
  /* border-top: 2px solid red;
  border-bottom: 2px solid blue; */
}